{
    "name": "bricklayer/structure",
    "description": "The core framework for Lay. The builder's builder",
    "keywords": ["framework", "lay", "lite framework", "lite php framework", "metaframework", "fast php framework"],
    "type": "library",
    "license": "GPL-3.0",
    "autoload": {
        "psr-4": {
            "BrickLayer\\Lay\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Osahenrumwen Aigbogun",
            "email": "leonardosahon@gmail.com"
        }
    ],
    "support": {
        "source": "https://github.com/PHPBrickLayer/structure",
        "issues": "https://github.com/PHPBrickLayer/structure/issues"
    },
    "require": {
        "php": "^8.1",
        "vlucas/phpdotenv": "^5.5.0",
        "wolfcast/browser-detection": "^2.9.7",
        "phpmailer/phpmailer": "^6.8.0",
        "ext-openssl": "*",
        "ext-mysqli": "*",
        "ext-json": "*",
        "ext-exif": "*",
        "ext-gd": "*",
        "ext-sodium": "*",
        "ext-fileinfo": "*",
        "ext-dom": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.3.2"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "npm-packages": {
            "terser": "5.15.1",
            "clean-css": "3.4.12"
        }
    }
}
